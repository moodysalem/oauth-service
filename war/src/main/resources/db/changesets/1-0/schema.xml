<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="Token Table" author="moody.salem">
        <createTable tableName="Token">
            <column name="id" type="bigint" autoIncrement="true"><constraints nullable="false" /></column>
            <column name="version" type="bigint" defaultValueNumeric="0"><constraints nullable="false" /></column>
        </createTable>
    </changeSet>

</databaseChangeLog>